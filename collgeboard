        tur.right(90)
        tur.fd(60)
    elif letter == "G":
        tur.pu()
        tur.fd(20)
        tur.right(90)
        tur.fd(30)
        tur.pd()
        tur.left(90)
        tur.fd(25)
        tur.right(90)
        tur.circle(-30,300)
        tur.pu()
        tur.left(30)
        tur.fd(60)


    elif letter == "H":
        tur.right(90)
        tur.fd(80)
        tur.right(180)
        tur.fd(40)
        tur.right(90)
        tur.fd(30)
        tur.right(90)
        tur.fd(40)
        tur.right(180)
        tur.fd(80)
        tur.pu()
        tur.right(90)
        tur.fd(10)
    elif letter == "I":
        tur.fd(70)
        tur.right(180)
        tur.fd(35)
        tur.right(270)
        tur.fd(80)
        tur.right(90)
        tur.fd(35)

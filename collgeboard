        tur.fd(80)
        tur.right(90)
        tur.fd(45)
    elif letter == "K":
        tur.right(90)
        tur.fd(80)
        tur.right(180)
        tur.fd(40)
        tur.right(45)
        tur.fd(60)
        tur.right(180)
        tur.fd(60)
        tur.right(-90)
        tur.fd(60)
        tur.pu()
        tur.right(180)
        tur.fd(60)
        tur.right(90)
        tur.fd(60)
        tur.right(45)
        tur.fd(10)
    elif letter == "L":
        tur.right(90)
        tur.fd(120)
        tur.right(-90)
        tur.fd(80)
        tur.pu()
        tur.right(180)
        tur.fd(80)
        tur.right(90)
        tur.fd(120)
        tur.right(90)
        tur.fd(90)
    elif letter == "M":
        tur.right(270)
        tur.fd(80)
        tur.right(180)
        tur.right(-45)
        tur.fd(40)
        tur.right(-90)
        tur.fd(40)
        tur.right(135)
        tur.fd(80)
        tur.pu()
        tur.right(180)

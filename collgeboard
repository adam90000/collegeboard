        tur.pu()
    elif letter == "V":
        tur.pu()
        tur.fd(20)

        tur.pd()
        tur.right(60)
        tur.fd(90)
        tur.left(120)
        tur.fd(90)
        tur.right(60)
	    
    elif letter == "W":
        tur.pu()
        tur.setposition(-100,0)
        tur.pd()
        tur.right(70)
        tur.fd(50)
        tur.right(-140)
        tur.fd(40)
        tur.right(140)
        tur.fd(40)
        tur.right(-140)
        tur.fd(50)
        tur.right(70)
        tur.pu()
        tur.fd(30)
        tur.left(90)
        tur.fd(30)
        tur.right(90)
        tur.pd()
    elif letter == "X":
        tur.left(45)
        tur.pu()
        tur.right(135)
        tur.fd(40)
        tur.pd()
        tur.right(135)
        tur.fd(65)
        tur.pu
        tur.backward(30)
        tur.right(50)
        tur.fd(40)
        tur.backward(30)
        tur.backward(30)
        tur.pu
        tur.right(45)
        tur.fd(50)
    elif letter == "Y":
        print()
    elif letter == "Z":
        tur.pd
        tur.fd(60)
        tur.right(80)
        tur.fd(60)
        tur.left(80)
        tur.fd(60)
        		

        
    elif letter == "Ax":
        # code here
        tur.forward(40)
		
    else:
        turtleLetter("box",tur)
	

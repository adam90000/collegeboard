        tur.fd(40)
        tur.left(90)
        tur.fd(55)
    elif letter == "R":
        tur.pu()
        tur.right(90)
        tur.fd(60)
        tur.left(90)
        
        tur.pd()
        tur.left(90)
        tur.fd(80)
        tur.right(90)  

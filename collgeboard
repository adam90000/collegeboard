        tur.left(180)
        tur.fd(60)
        tur.right(50)
        tur.fd(40)
        tur.right(90)
        tur.fd(40)
        tur.pu()
     
 
    elif letter == "S":
        tur.pu()
        tur.fd(60)
        tur.right(180)
